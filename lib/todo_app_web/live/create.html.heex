<div>
  <.form :let={f} for={@form} phx-target={@myself} phx-change="validate" phx-submit="save">
    <.input type="text" placeholder="Enter your Name" label="Name" field={f[:name]} />
    <.input type="text" placeholder="Enter your Status" label="Status" field={f[:status]} />
    <.button type="submit">Save</.button>
  </.form>
</div>
